{
    "name": "stylelint-config",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "packages/stylelint-config/src",
    "projectType": "library",
    "targets": {
        "build": {
            "executor": "@nrwl/js:tsc",
            "outputs": [
                "{options.outputPath}"
